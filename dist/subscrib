issuerAccountReferenceId= request.json['issuerAccountReferenceId'] 
authenticationToken= request.json['authenticationToken'] 
hceTechnicalId= request.json['hceTechnicalId']
response, status_code = checkUserAuthentification8(authenticationToken=authenticationToken,issuerAccountReferenceId=issuerAccountReferenceId)  
request = requests.post( 'https://127.0.0.1:7776/checkUserAuthentification' , json={'authenticationToken': authenticationToken, 'issuerAccountReferenceId': issuerAccountReferenceId}, verify=False)
status_code = requestsent.status_code
response = request.json()
if str(status_code) == str(200) : 
    request = requests.post( 'https://127.0.0.1:7776/digitize'  , json={'hceTechnicalId': hceTechnicalId, 'externalUserId': issuerAccountReferenceId}, verify=False)
    status_code = requestsent.status_code
    response = request.json()
    if str(status_code) == str(200) : 
        traceId = response['traceId']
        request = requests.post( 'https://127.0.0.1:7776/digitizeStatus' , json={'issuerAccountReferenceId': issuerAccountReferenceId, 'traceId': traceId}, verify=False)
        
    
	